{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/react-src/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": ["app.*", "os.showNotification", "debug.log"],
  "modes": {
    "window": {
      "title": "localchat",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/react-src/src/assets/react.svg",
      "enableInspector": true
    }
  },
  "cli": {
    "binaryName": "localchat",
    "extensionsPath": "/extensions/",
    "resourcesPath": "/react-src/dist/",
    "frontendLibrary": {
      "patchFile": "/react-src/index.html",
      "devUrl": "http://localhost:3000",
      "projectPath": "/react-src/",
      "initCommand": "npm install",
      "devCommand": "vite --host",
      "buildCommand": "tsc && vite build"
    },
    "binaryVersion": "5.0.0"
  }
}
